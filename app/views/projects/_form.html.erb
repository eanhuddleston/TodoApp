<%= form_for @project do |f| %>

  <p>
    <%= f.label :title, "Title:" %> <br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :purpose, "Purpose:" %> <br>
    <%= f.text_field :purpose %>
  </p>
  <p>
    <%= f.collection_select :team_id, Team.all, :id, :name %>
  </p>
  <p>
    <%= f.submit %> 
  </p>

<% end %>