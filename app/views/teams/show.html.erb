<h1><%= @team.name %></h1>
<br>

<h4>Members</h4>
<% @team.users.each do |user| %>
  <%= link_to "#{user.fname} #{user.lname}", user_url(user) %>
  <br>
<% end %>

<h4>Projects</h4>
<% @team.projects.each do |project| %>
  <%= link_to project.title, project_url(project) %>
  <br>
<% end %>

<h4>Links</h4>
<ul>
  <li><%= link_to "All teams", teams_url %></li>
  <li><%= link_to "All projects", projects_url %></li>
</ul>